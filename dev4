
#include<stdio.h>
void val(int a, int b);
int main()
{
    char a;
    int b;
    printf("a = ");
    scanf("%c",&a);
    printf("b = ");
    scanf("%d",&b);
    val(a,b);
    
}

void val(int a, int b)
{
    
    if(a=='c' || a=='C')
    {
        if(b>=1 && b<=3){
        printf("Welcome to CCD!\nEnjoy your ");
        if(b==1) puts("Espresso Coffee ");
        else if(b==2) puts("Cappuccino Coffee");
        else if(b==3) puts("Latte Coffee");
        }
        else
        {
            printf("INVALID OUTPUT!");
        }
    }
    else if(a=='T' || a=='t')
    {
        if(b>=1 && b<=8){
        printf("Welcome to CCD!\nEnjoy your ");
        if(b==1) puts("Plain Tea ");
        else if(b==2) puts("Assam Tea");
        else if(b==3) puts("Ginger Tea");
        else if(b==4) puts("Cardamom Tea");
        else if(b==5) puts("Masala Tea");
        else if(b==6) puts("Lemon Tea");
        else if(b==7) puts("Green Tea");
        else if(b==8) puts("Organic Darjeeling Tea");
        }
        else
        {
            printf("INVALID OUTPUT!");
        }
    }
    else if(a=='s' || a=='S')
    {
        if(b>=1 && b<=4){
        printf("Welcome to CCD!\nEnjoy your ");
        if(b==1) puts("Hot and Sour Soup ");
        else if(b==2) puts("Veg Corn Soup");
        else if(b==3) puts("Tomato Soup");
        else if(b==4) puts("Spicy Tomato Soup");
        }
        else
        {
            printf("INVALID OUTPUT!");
        }
    }
    else if(a=='b' || a=='B')
    {
        if(b>=1 && b<=3){
        printf("Welcome to CCD!\nEnjoy your ");
        if(b==1) puts("Hot Chocolate Drink ");
        else if(b==2) puts("Badam Drink");
        else if(b==3) puts("Badam-Pista Drink");
        }
        else
        {
            printf("INVALID OUTPUT!");
        }
        
    }
    else
    {
        printf("INVALID OUTPUT!");
    }
    
}
